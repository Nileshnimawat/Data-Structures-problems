
class rectangle {
 double length;
 double width;
 rectangle() {
 length = 0;
        width = 0;
    }

    rectangle(double length, double width) {
        System.out.println("Parameterised constructor used in rectangle");
        this.length = length;
        this.width = width;
    }

    double perimeter() {
        return 2 * (length + width);
    }

    double area() {
        return (length * width);
    }
}

class box extends rectangle {
    double height;
    box() {
        super();
        height = 0;
    }
    box(double length, double height, double width) {
        super(length, width); // super used
        System.out.println("Parameterised constructor used in box and super used");
        this.height = height;
    }

    double volume() {
        return length * height * width;
    }
}

class rectangle_box {
 public static void main(String[] args) {
    s
        double vol1, ar1, pr1;
        rectangle i3 = new rectangle(4, 5);
        ar1 = i3.area();
        pr1 = i3.perimeter();
        System.out.println("Area of rectangle1 = " + ar1);
        System.out.println("Perimeter of rectangle2 = " + pr1);

        box i1 = new box(34, 56, 78);
        vol1 = i1.volume();
        System.out.println("Volume of box1 = " + vol1);
    }
}
